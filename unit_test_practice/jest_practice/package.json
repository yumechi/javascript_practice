{
    "name": "jest_practice",
    "version": "1.0.0",
    "description": "Jest practice project with TypeScript support",
    "main": "index.js",
    "scripts": {
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage"
    },
    "directories": {
        "test": "__tests__"
    },
    "dependencies": {
        "@types/node": "^22.10.2"
    },
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "jest": "^29.7.0",
        "ts-jest": "^29.2.5",
        "typescript": "^5.7.2"
    },
    "keywords": ["jest", "testing", "typescript"],
    "author": "",
    "license": "ISC",
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx"
        ],
        "testMatch": [
            "**/__tests__/**/*.(ts|tsx|js)"
        ],
        "collectCoverageFrom": [
            "script/**/*.(ts|tsx)",
            "!script/**/*.d.ts"
        ]
    }
}
